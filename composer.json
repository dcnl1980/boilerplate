{
    "name": "php-http/boilerplate",
    "description": "Boilerplate PHP HTTP library",
    "license": "MIT",
    "keywords": ["boilerplate", "http"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Http\\Boilerplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Http\\Boilerplate\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "",
        "test-ci": ""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
